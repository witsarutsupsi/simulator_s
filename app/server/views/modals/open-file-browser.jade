.modal-open-file-browser.modal.fade
	.modal-dialog
		.modal-content
			.modal-header.modal-h
				img(src="/images/SVG/modalicon/m1.svg", style="height:40px; display:inline;")
				h4(style="display:inline; vertical-align:middle;")
			.modal-body.modal-b(style="padding-bottom: 5px !important")
				//- .list-group(style="margin-bottom: 0px !important")#file-list
				.custom-file
					input#customFile(type='file')
					//- label.custom-file-label(for='customFile') Choose file
			div(style="text-align: right;padding-right: 20px").modal-b
				p(style="display:inline;")
					input(type="checkbox" style="display:inline;")#insert-checkbox
					div(style="display: inline;vertical-align: middle;")#insert-text Insert Mode
			.modal-footer.modal-f
				button#btn-cancel(data-dismiss="modal").cancel.btn.btn-danger Cancel
				button#btn-ok.btn.btn-success OK
